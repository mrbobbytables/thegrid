{
    "id": "/nginx-jm",
    "instances": 2,
    "cpus": 0.25,
    "mem": 32,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "registry.marathon.mesos:31111/nginx-jm:latest",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ]
        }
    },
    "healthChecks": [
        {
            "protocol": "HTTP"
        }
    ],
    "backoffSeconds": 2,
    "backoffFactor": 1.25,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0.5,
        "maximumOverCapacity": 0.5
    }
}
