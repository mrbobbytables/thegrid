{
    "id": "/nginx",
    "instances": 1,
    "cpus": 0.25,
    "mem": 64,
    "ports": [
      80
    ],
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "nginx",
            "network": "BRIDGE",
            "portMappings": [
              { "containerPort": 80, "hostPort": 0, "protocol": "tcp" }
            ]
        }
    }
}

